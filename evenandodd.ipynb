   "source": [
    "s=[1,2,3,4,5,6]\n",
    "length=len(s)\n",
    "even=0\n",
    "odd=0\n",
    "while (length>=1):\n",
    "    num=s[length-1]\n",
    "    y=num%2\n",
    "    if(y==0):\n",
    "        even=even+1\n",
    "    else:\n",
    "        odd=odd+1\n",
    "    length=length-1\n",
    "print(\"Number of odd number is:\",odd)\n",
    "print(\"Number of even number is:\",even)\n"
   ]
